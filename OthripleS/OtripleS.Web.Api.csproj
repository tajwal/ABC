<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>OtripleS.Web.Api</AssemblyName>
    <RootNamespace>OtripleS.Web.Api</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Services\Classrooms\ClassroomService.Validations.cs" />
    <Compile Remove="Services\Courses\CourseService.Exception.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Services\Classrooms\ClassroomService.Validations.cs" />
    <None Include="Services\Courses\CourseService.Exception.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="EFxceptions" Version="0.4.4" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="3.1.6" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="3.1.6" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Tools" Version="3.1.6">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="RESTFulSense" Version="1.4.0" />
  </ItemGroup>


</Project>
